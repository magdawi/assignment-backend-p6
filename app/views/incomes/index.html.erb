<p id="notice"><%= notice %></p>

<h1>Listing Incomes</h1>

<table>
  <thead>
    <tr>
      <th>Note</th>
      <th>Date</th>
      <th>Sum</th>
      <th>Category</th>
      <th>Pocket</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @incomes.each do |income| %>
      <tr>
        <td><%= income.note %></td>
        <td><%= income.date %></td>
        <td><%= income.sum %></td>
        <td><%= income.category.name %></td>
        <td><%= income.pocket.name %></td>
        <td><%= income.user.name %></td>
        <td><%= link_to 'Show', income %></td>
        <td><%= link_to 'Edit', edit_income_path(income) %></td>
        <td><%= link_to 'Destroy', income, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Income', new_income_path %>
